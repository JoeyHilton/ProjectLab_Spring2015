<h1>Users</h1>
<% @users.each do |user| %>
<div class="col-md-4">
    <div class='panel panel-default'>
      <div class="panel-body">
        <h2><%= user.first_name %> <%= user.last_name %></h2>
        <h3><%= user.email %></h3>
       
        <%= button_to 'Show', user_path(user.id), class: "btn btn-primary", method: :get %>
      </div>
    </div>
  </div>  
<% end %>    